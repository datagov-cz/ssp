@prefix a-popis-dat-pojem: <http://onto.fel.cvut.cz/ontologies/slovník/agendový/popis-dat/pojem/> .
@prefix bibo: <http://purl.org/ontology/bibo/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix vann: <http://purl.org/vocab/vann/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix z-sgov: <https://slovník.gov.cz/základní/> .
@prefix z-sgov-pojem: <https://slovník.gov.cz/základní/pojem/> .

z-sgov:model a a-popis-dat-pojem:model, owl:Ontology;
  <http://purl.org/dc/elements/1.1/description> "Základní slovník popisuje základní vztahy."@cs;
  <http://purl.org/dc/elements/1.1/rights> <https://creativecommons.org/licenses/by-sa/4.0>;
  <http://purl.org/dc/elements/1.1/title> "Basic Vocabulary (Z-SGoV) - model"@en, "Základní slovník (Z-SGoV) - model"@cs;
  dcterms:created "03.09.2018", "2020-05-21";
  dcterms:description "Základní slovník popisuje základní vztahy."@cs;
  dcterms:rights <https://creativecommons.org/licenses/by-sa/4.0>;
  dcterms:title "Basic Vocabulary (Z-SGoV) - model"@en, "Základní slovník (Z-SGoV) - model"@cs;
  bibo:status "Specification"@en, "Specifikace"@cs;
  vann:preferredNamespacePrefix "z-sgov-pojem";
  vann:preferredNamespaceUri "https://slovník.gov.cz/základní/pojem/";
  rdfs:label "Basic Vocabulary (Z-SGoV) - model"@en, "Základní slovník (Z-SGoV) - model"@cs;
  owl:imports z-sgov:glosář;
  owl:versionIRI <https://slovník.gov.cz/základní/model/verze/1.0.0>, <https://slovník.gov.cz/základní/model/verze/1.0.1> .

z-sgov-pojem:typ-objektu a owl:Class;
  rdfs:subClassOf z-sgov-pojem:typ-proměnného-prvku;
  z-sgov-pojem:charakterizuje z-sgov-pojem:objekt .

z-sgov-pojem:vlastnost a owl:Class, owl:DatatypeProperty, z-sgov-pojem:typ-vlastnosti;
  rdfs:domain z-sgov-pojem:proměnný-prvek;
  rdfs:subClassOf z-sgov-pojem:proměnný-prvek, [ a owl:Restriction;
      owl:minQualifiedCardinality "1"^^xsd:long;
      owl:onClass z-sgov-pojem:proměnný-prvek;
      owl:onProperty z-sgov-pojem:je-vlastností
    ], [ a owl:Restriction;
      owl:minQualifiedCardinality 1;
      owl:onClass z-sgov-pojem:proměnný-prvek;
      owl:onProperty z-sgov-pojem:je-vlastností
    ], [ a owl:Restriction;
      owl:allValuesFrom z-sgov-pojem:proměnný-prvek;
      owl:onProperty z-sgov-pojem:je-vlastností
    ], [ a owl:Restriction;
      owl:allValuesFrom z-sgov-pojem:proměnný-prvek;
      owl:onProperty z-sgov-pojem:je-vlastností
    ], [ a owl:Restriction;
      owl:maxQualifiedCardinality "1"^^xsd:long;
      owl:onClass z-sgov-pojem:proměnný-prvek;
      owl:onProperty z-sgov-pojem:je-vlastností
    ], [ a owl:Restriction;
      owl:maxQualifiedCardinality 1;
      owl:onClass z-sgov-pojem:proměnný-prvek;
      owl:onProperty z-sgov-pojem:je-vlastností
    ] .

z-sgov-pojem:typ-vlastnosti a owl:Class, z-sgov-pojem:typ;
  rdfs:subClassOf z-sgov-pojem:typ-proměnného-prvku;
  z-sgov-pojem:charakterizuje z-sgov-pojem:vlastnost .

z-sgov-pojem:vztah a owl:Class, owl:ObjectProperty, z-sgov-pojem:typ-vztahu;
  rdfs:domain z-sgov-pojem:proměnný-prvek;
  rdfs:subClassOf z-sgov-pojem:proměnný-prvek, [ a owl:Restriction;
      owl:minQualifiedCardinality "2"^^xsd:nonNegativeInteger;
      owl:onClass z-sgov-pojem:proměnný-prvek;
      owl:onProperty z-sgov-pojem:má-vztažený-prvek
    ], [ a owl:Restriction;
      owl:allValuesFrom z-sgov-pojem:proměnný-prvek;
      owl:onProperty z-sgov-pojem:má-vztažený-prvek-1
    ], [ a owl:Restriction;
      owl:allValuesFrom z-sgov-pojem:proměnný-prvek;
      owl:onProperty z-sgov-pojem:má-vztažený-prvek
    ], [ a owl:Restriction;
      owl:allValuesFrom z-sgov-pojem:proměnný-prvek;
      owl:onProperty z-sgov-pojem:má-vztažený-prvek-2
    ], [ a owl:Restriction;
      owl:minQualifiedCardinality "2"^^xsd:long;
      owl:onClass z-sgov-pojem:proměnný-prvek;
      owl:onProperty z-sgov-pojem:má-vztažený-prvek-1
    ], [ a owl:Restriction;
      owl:allValuesFrom z-sgov-pojem:proměnný-prvek;
      owl:onProperty z-sgov-pojem:má-vztažený-prvek
    ] .

z-sgov-pojem:typ-vztahu a owl:Class;
  rdfs:subClassOf z-sgov-pojem:typ-proměnného-prvku;
  z-sgov-pojem:charakterizuje z-sgov-pojem:vztah .

z-sgov-pojem:objekt a owl:Class, z-sgov-pojem:typ-objektu;
  rdfs:subClassOf z-sgov-pojem:proměnný-prvek .

z-sgov-pojem:typ-události a owl:Class;
  rdfs:subClassOf z-sgov-pojem:typ;
  z-sgov-pojem:charakterizuje z-sgov-pojem:událost .

z-sgov-pojem:role a owl:Class;
  rdfs:subClassOf z-sgov-pojem:sortál .

z-sgov-pojem:typ a owl:Class;
  rdfs:subClassOf z-sgov-pojem:pasivní-objekt .

z-sgov-pojem:druh a owl:Class;
  rdfs:subClassOf z-sgov-pojem:sortál .

z-sgov-pojem:kategorie a owl:Class;
  rdfs:subClassOf z-sgov-pojem:nesortál .

z-sgov-pojem:sortál a owl:Class;
  rdfs:subClassOf z-sgov-pojem:typ-proměnného-prvku .

z-sgov-pojem:mixin-rolí a owl:Class;
  rdfs:subClassOf z-sgov-pojem:nesortál .

z-sgov-pojem:typ-kvalitativní-vlastnosti a owl:Class, z-sgov-pojem:typ;
  rdfs:subClassOf z-sgov-pojem:typ-vlastnosti .

z-sgov-pojem:časová-specifikace a owl:Class, z-sgov-pojem:typ;
  rdfs:subClassOf z-sgov-pojem:pasivní-objekt, z-sgov-pojem:prvek .

z-sgov-pojem:pasivní-objekt a owl:Class;
  rdfs:subClassOf z-sgov-pojem:objekt .

z-sgov-pojem:typ-proměnného-prvku a owl:Class, z-sgov-pojem:typ;
  rdfs:subClassOf z-sgov-pojem:typ .

z-sgov-pojem:poddruh a owl:Class;
  rdfs:subClassOf z-sgov-pojem:sortál .

z-sgov-pojem:nesortál a owl:Class;
  rdfs:subClassOf z-sgov-pojem:typ-proměnného-prvku .

z-sgov-pojem:agent a owl:Class;
  rdfs:subClassOf z-sgov-pojem:objekt;
  owl:disjointWith z-sgov-pojem:pasivní-objekt .

z-sgov-pojem:je-vlastností a owl:ObjectProperty;
  rdfs:domain z-sgov-pojem:vlastnost;
  rdfs:range z-sgov-pojem:proměnný-prvek .

z-sgov-pojem:má-vlastnost a owl:ObjectProperty;
  rdfs:domain z-sgov-pojem:proměnný-prvek;
  rdfs:range z-sgov-pojem:vlastnost .

z-sgov-pojem:akce a owl:Class;
  rdfs:subClassOf z-sgov-pojem:událost, [ a owl:Restriction;
      owl:minQualifiedCardinality "1"^^xsd:long;
      owl:onClass z-sgov-pojem:agent;
      owl:onProperty z-sgov-pojem:je-vykonána
    ], [ a owl:Restriction;
      owl:maxQualifiedCardinality 1;
      owl:onClass z-sgov-pojem:agent;
      owl:onProperty z-sgov-pojem:je-vykonána
    ], [ a owl:Restriction;
      owl:allValuesFrom z-sgov-pojem:agent;
      owl:onProperty z-sgov-pojem:je-vykonána
    ], [ a owl:Restriction;
      owl:allValuesFrom z-sgov-pojem:agent;
      owl:onProperty z-sgov-pojem:je-vykonána
    ], [ a owl:Restriction;
      owl:maxQualifiedCardinality "1"^^xsd:long;
      owl:onClass z-sgov-pojem:agent;
      owl:onProperty z-sgov-pojem:je-vykonána
    ], [ a owl:Restriction;
      owl:minQualifiedCardinality 1;
      owl:onClass z-sgov-pojem:agent;
      owl:onProperty z-sgov-pojem:je-vykonána
    ] .

z-sgov-pojem:událost a owl:Class;
  rdfs:subClassOf z-sgov-pojem:neměnný-prvek, [ a owl:Restriction;
      owl:minQualifiedCardinality 1;
      owl:onClass z-sgov-pojem:okamžitý-stav-proměnného-prvku;
      owl:onProperty z-sgov-pojem:má-koncový-stav
    ], [ a owl:Restriction;
      owl:allValuesFrom z-sgov-pojem:proměnný-prvek;
      owl:onProperty z-sgov-pojem:má-účastníka
    ], [ a owl:Restriction;
      owl:minQualifiedCardinality "1"^^xsd:long;
      owl:onClass z-sgov-pojem:okamžitý-stav-proměnného-prvku;
      owl:onProperty z-sgov-pojem:má-účastníka
    ], [ a owl:Restriction;
      owl:minQualifiedCardinality 1;
      owl:onClass z-sgov-pojem:okamžitý-stav-proměnného-prvku;
      owl:onProperty z-sgov-pojem:má-počáteční-stav
    ], [ a owl:Restriction;
      owl:minQualifiedCardinality 1;
      owl:onClass z-sgov-pojem:proměnný-prvek;
      owl:onProperty z-sgov-pojem:má-účastníka
    ], [ a owl:Restriction;
      owl:allValuesFrom z-sgov-pojem:okamžitý-stav-proměnného-prvku;
      owl:onProperty z-sgov-pojem:má-účastníka
    ], [ a owl:Restriction;
      owl:allValuesFrom z-sgov-pojem:okamžitý-stav-proměnného-prvku;
      owl:onProperty z-sgov-pojem:má-počáteční-stav
    ], [ a owl:Restriction;
      owl:allValuesFrom z-sgov-pojem:okamžitý-stav-proměnného-prvku;
      owl:onProperty z-sgov-pojem:má-koncový-stav
    ], [ a owl:Restriction;
      owl:allValuesFrom z-sgov-pojem:okamžitý-stav-proměnného-prvku;
      owl:onProperty z-sgov-pojem:má-počáteční-stav
    ], [ a owl:Restriction;
      owl:minQualifiedCardinality "1"^^xsd:long;
      owl:onClass z-sgov-pojem:okamžitý-stav-proměnného-prvku;
      owl:onProperty z-sgov-pojem:má-počáteční-stav
    ], [ a owl:Restriction;
      owl:allValuesFrom z-sgov-pojem:okamžitý-stav-proměnného-prvku;
      owl:onProperty z-sgov-pojem:má-koncový-stav
    ], [ a owl:Restriction;
      owl:minQualifiedCardinality "1"^^xsd:long;
      owl:onClass z-sgov-pojem:okamžitý-stav-proměnného-prvku;
      owl:onProperty z-sgov-pojem:má-koncový-stav
    ] .

z-sgov-pojem:je-ve-vztahu a owl:ObjectProperty;
  rdfs:domain z-sgov-pojem:proměnný-prvek;
  rdfs:range z-sgov-pojem:vztah .

z-sgov-pojem:má-vztažený-prvek a owl:ObjectProperty;
  rdfs:domain z-sgov-pojem:vztah;
  rdfs:range z-sgov-pojem:proměnný-prvek .

z-sgov-pojem:má-vztažený-prvek-1 a owl:ObjectProperty;
  rdfs:domain z-sgov-pojem:vztah;
  rdfs:range z-sgov-pojem:proměnný-prvek;
  rdfs:subPropertyOf z-sgov-pojem:má-vztažený-prvek .

z-sgov-pojem:má-vztažený-prvek-2 a owl:ObjectProperty;
  rdfs:domain z-sgov-pojem:vztah;
  rdfs:range z-sgov-pojem:proměnný-prvek;
  rdfs:subPropertyOf z-sgov-pojem:má-vztažený-prvek .

z-sgov-pojem:proměnný-prvek a owl:Class, z-sgov-pojem:typ-proměnného-prvku;
  rdfs:subClassOf z-sgov-pojem:prvek, [ a owl:Restriction;
      owl:allValuesFrom z-sgov-pojem:vlastnost;
      owl:onProperty z-sgov-pojem:má-vlastnost
    ], [ a owl:Restriction;
      owl:allValuesFrom z-sgov-pojem:vztah;
      owl:onProperty z-sgov-pojem:je-ve-vztahu
    ], [ a owl:Restriction;
      owl:allValuesFrom z-sgov-pojem:vztah;
      owl:onProperty z-sgov-pojem:je-ve-vztahu
    ], [ a owl:Restriction;
      owl:allValuesFrom z-sgov-pojem:vlastnost;
      owl:onProperty z-sgov-pojem:má-vlastnost
    ] .

z-sgov-pojem:vznik-proměnného-prvku a owl:Class;
  rdfs:subClassOf z-sgov-pojem:událost .

z-sgov-pojem:fáze a owl:Class;
  rdfs:subClassOf z-sgov-pojem:sortál .

z-sgov-pojem:prvek a owl:Class, z-sgov-pojem:typ .

z-sgov-pojem:množství a owl:Class;
  rdfs:subClassOf z-sgov-pojem:pasivní-objekt .

z-sgov-pojem:typ-množství a owl:Class;
  rdfs:subClassOf z-sgov-pojem:sortál .

z-sgov-pojem:mixin-fází a owl:Class;
  rdfs:subClassOf z-sgov-pojem:nesortál .

z-sgov-pojem:typ-modální-vlastnosti a owl:Class;
  rdfs:subClassOf z-sgov-pojem:typ-vlastnosti .

z-sgov-pojem:neměnný-prvek a owl:Class, z-sgov-pojem:typ;
  rdfs:subClassOf z-sgov-pojem:prvek, [ a owl:Restriction;
      owl:minQualifiedCardinality "1"^^xsd:long;
      owl:onClass z-sgov-pojem:časová-specifikace;
      owl:onProperty z-sgov-pojem:má-časové-určení
    ], [ a owl:Restriction;
      owl:maxQualifiedCardinality 1;
      owl:onClass z-sgov-pojem:prostorový-objekt;
      owl:onProperty z-sgov-pojem:má-prostorové-určení
    ], [ a owl:Restriction;
      owl:allValuesFrom z-sgov-pojem:prostorový-objekt;
      owl:onProperty z-sgov-pojem:má-prostorové-určení
    ], [ a owl:Restriction;
      owl:minQualifiedCardinality 1;
      owl:onClass z-sgov-pojem:časová-specifikace;
      owl:onProperty z-sgov-pojem:má-časové-určení
    ], [ a owl:Restriction;
      owl:maxQualifiedCardinality "1"^^xsd:long;
      owl:onClass z-sgov-pojem:prostorový-objekt;
      owl:onProperty z-sgov-pojem:má-prostorové-určení
    ], [ a owl:Restriction;
      owl:allValuesFrom z-sgov-pojem:prostorový-objekt;
      owl:onProperty z-sgov-pojem:má-prostorové-určení
    ], [ a owl:Restriction;
      owl:minQualifiedCardinality "1"^^xsd:long;
      owl:onClass z-sgov-pojem:prostorový-objekt;
      owl:onProperty z-sgov-pojem:má-prostorové-určení
    ], [ a owl:Restriction;
      owl:allValuesFrom z-sgov-pojem:časová-specifikace;
      owl:onProperty z-sgov-pojem:má-časové-určení
    ], [ a owl:Restriction;
      owl:maxQualifiedCardinality 1;
      owl:onClass z-sgov-pojem:časová-specifikace;
      owl:onProperty z-sgov-pojem:má-časové-určení
    ], [ a owl:Restriction;
      owl:maxQualifiedCardinality "1"^^xsd:long;
      owl:onClass z-sgov-pojem:časová-specifikace;
      owl:onProperty z-sgov-pojem:má-časové-určení
    ], [ a owl:Restriction;
      owl:minQualifiedCardinality 1;
      owl:onClass z-sgov-pojem:prostorový-objekt;
      owl:onProperty z-sgov-pojem:má-prostorové-určení
    ], [ a owl:Restriction;
      owl:allValuesFrom z-sgov-pojem:časová-specifikace;
      owl:onProperty z-sgov-pojem:má-časové-určení
    ] .

z-sgov-pojem:okamžitý-stav-proměnného-prvku a owl:Class, z-sgov-pojem:typ;
  rdfs:subClassOf z-sgov-pojem:neměnný-prvek, z-sgov-pojem:proměnný-prvek, [ a owl:Restriction;
      owl:maxQualifiedCardinality 1;
      owl:onClass z-sgov-pojem:proměnný-prvek;
      owl:onProperty z-sgov-pojem:je-stavem
    ], [ a owl:Restriction;
      owl:minQualifiedCardinality 1;
      owl:onClass z-sgov-pojem:proměnný-prvek;
      owl:onProperty z-sgov-pojem:je-stavem
    ], [ a owl:Restriction;
      owl:allValuesFrom z-sgov-pojem:proměnný-prvek;
      owl:onProperty z-sgov-pojem:je-stavem
    ];
  owl:disjointWith z-sgov-pojem:událost .

z-sgov-pojem:je-vykonána a owl:ObjectProperty;
  rdfs:domain z-sgov-pojem:akce;
  rdfs:range z-sgov-pojem:agent .

z-sgov-pojem:mixin a owl:Class;
  rdfs:subClassOf z-sgov-pojem:nesortál .

z-sgov-pojem:má-koncový-stav a owl:ObjectProperty;
  rdfs:domain z-sgov-pojem:událost;
  rdfs:range z-sgov-pojem:okamžitý-stav-proměnného-prvku .

z-sgov-pojem:má-počáteční-stav a owl:ObjectProperty;
  rdfs:domain z-sgov-pojem:událost;
  rdfs:range z-sgov-pojem:okamžitý-stav-proměnného-prvku .

z-sgov-pojem:má-prostorové-určení a owl:ObjectProperty;
  rdfs:domain z-sgov-pojem:neměnný-prvek;
  rdfs:range z-sgov-pojem:prostorový-objekt .

z-sgov-pojem:prostorový-objekt a owl:Class;
  rdfs:subClassOf z-sgov-pojem:pasivní-objekt .

z-sgov-pojem:má-účastníka a owl:ObjectProperty;
  rdfs:domain z-sgov-pojem:událost;
  rdfs:label "má účastníka"@cs;
  rdfs:range z-sgov-pojem:okamžitý-stav-proměnného-prvku, z-sgov-pojem:proměnný-prvek .

z-sgov-pojem:má-časové-určení a owl:ObjectProperty;
  rdfs:domain z-sgov-pojem:neměnný-prvek;
  rdfs:label "má časové určení"@cs;
  rdfs:range z-sgov-pojem:časová-specifikace .

z-sgov-pojem:typ-módu a owl:Class;
  rdfs:subClassOf z-sgov-pojem:typ-vlastnosti .

z-sgov-pojem:změna-proměnného-prvku a owl:Class;
  rdfs:subClassOf z-sgov-pojem:událost .

z-sgov-pojem:zánik-proměnného-prvku a owl:Class;
  rdfs:subClassOf z-sgov-pojem:událost .

z-sgov-pojem:je-stavem a owl:ObjectProperty;
  rdfs:domain z-sgov-pojem:okamžitý-stav-proměnného-prvku;
  rdfs:range z-sgov-pojem:proměnný-prvek .

z-sgov-pojem:charakterizuje a owl:AnnotationProperty;
  rdfs:domain z-sgov-pojem:typ;
  rdfs:range z-sgov-pojem:prvek .

skos:prefLabel rdfs:subPropertyOf rdfs:label .

[] a owl:AllDisjointClasses;
  owl:members (z-sgov-pojem:objekt z-sgov-pojem:vlastnost z-sgov-pojem:vztah) .

[] a owl:AllDisjointClasses;
  owl:members (z-sgov-pojem:neměnný-prvek z-sgov-pojem:proměnný-prvek z-sgov-pojem:časová-specifikace) .
