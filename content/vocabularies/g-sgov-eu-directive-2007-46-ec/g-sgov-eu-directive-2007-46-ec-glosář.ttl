  skos:altLabel "Hnací jednotka"@cs, "Pohon"@cs, "Power plant"@en, "Propulsion"@en;
  skos:inScheme g-sgov-eu-directive-2007-46-ec:glosář;
  skos:prefLabel "Engine"@en, "Motor"@cs .

