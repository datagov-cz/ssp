  skos:inScheme g-sgov-iso-iec-tr-270192013-07-15:glosář;
  skos:prefLabel "Prohlášení o použitelnosti"@cs, "Statement of Applicability"@en .

g-sgov-iso-iec-tr-270192013-07-15-pojem:systém-řízení-procesů a skos:Concept;
  dcterms:source "3.12";
  skos:definition "system that serves to control and monitor the generation, transmission, storage and distribution of electric power, gas and heat in combination with the control of supporting processes"@en;
  skos:inScheme g-sgov-iso-iec-tr-270192013-07-15:glosář;
  skos:prefLabel "Process Control Systém"@en, "Systém řízení procesů"@cs .
