g-sgov-iso-iec-tr-270192013-07-15-pojem:blackout a skos:Concept;
  dcterms:source "3.1";
  skos:definition "widespread electrical power outage"@en;
  skos:inScheme g-sgov-iso-iec-tr-270192013-07-15:glosář;
  skos:prefLabel "Blackout"@cs, "Blackout"@en .

g-sgov-iso-iec-tr-270192013-07-15-pojem:dohledové-centrum a skos:Concept;
  dcterms:source "3.2";
  skos:definition "team of security experts to support the handling of information security incidents"@en,
    "tým bezpečnostních expertů na podporu řešení incidentů informační bezpečnosti "@cs;
  skos:inScheme g-sgov-iso-iec-tr-270192013-07-15:glosář;
  skos:prefLabel "Computer Emergency Response Team"@en, "Dohledové centrum "@cs .

g-sgov-iso-iec-tr-270192013-07-15-pojem:kritická-infrastruktura a skos:Concept;
  dcterms:source "3.3";
  skos:definition "organizations and facilities that are essential for the functioning of society and the economy as a whole NOTE　A failure or malfunction of such organizations and facilities would result in sustained supply shortfalls, make a significant impact on public security and have other wide ranging impacts."@en;
  skos:inScheme g-sgov-iso-iec-tr-270192013-07-15:glosář;
  skos:prefLabel "Critical infrastructure"@en, "Kritická infrastruktura"@cs .

g-sgov-iso-iec-tr-270192013-07-15-pojem:prohlášení-o-použitelnosti a skos:Concept;
  dcterms:source "3.16";
