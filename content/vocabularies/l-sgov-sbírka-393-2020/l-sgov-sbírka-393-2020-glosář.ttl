l-sgov-sbírka-393-2020-pojem:doprava a skos:Concept;
  dcterms:source "ČSN 018500 - SDT";
  skos:definition "úmyslný pohyb (jízda, plavba, let) dopravních prostředků po dopravních cestách nebo činnost dopravních zařízení; odvětví národního hospodářství, které zajišťuje a uskutečňuje přemisťování osob a věcí"@cs;
  skos:inScheme l-sgov-sbírka-393-2020:glosář;
  skos:prefLabel "doprava"@cs;
  skos:scopeNote ""@cs .

l-sgov-sbírka-393-2020-pojem:stavba a skos:Concept;
  dcterms:source "ISO67047-1, ČSN EN 1990 - SDT";
  skos:definition "vše, co bylo postaveno nebo je výsledkem stavební činnosti"@cs;
  skos:inScheme l-sgov-sbírka-393-2020:glosář;
  skos:prefLabel "stavba"@cs;
  skos:related l-sgov-sbírka-393-2020-pojem:identifikační-číslo-stavby;
  skos:scopeNote ""@cs .

l-sgov-sbírka-393-2020-pojem:inženýrská-stavba a skos:Concept;
  dcterms:source "ČSN 730031 - SDT";
  skos:broader l-sgov-sbírka-393-2020-pojem:stavba;
  skos:definition "produkt stavební činnosti, který umožňuje realizaci výrobních, dopravních, spojových a jiných procesů (most, zemní hráz, chladicí věž ap.) nebo určený pro skladování hotových výrobků (nádrž, zásobník ap.)"@cs;
  skos:inScheme l-sgov-sbírka-393-2020:glosář;
  skos:prefLabel "inženýrská stavba"@cs;
  skos:related l-sgov-sbírka-393-2020-pojem:identifikační-číslo-stavby;
  skos:scopeNote ""@cs .

l-sgov-sbírka-393-2020-pojem:jiná-stavba a skos:Concept;
  dcterms:source "vlastní rozšíření";
  skos:broader l-sgov-sbírka-393-2020-pojem:stavba;
  skos:definition "stavby doplňující stavbu dráhy jako stavby na dráze o objekty potřebné k podpoře funkcí dráhy (pozemní stavby obecnějšího významu – např. bytové)"@cs;
  skos:inScheme l-sgov-sbírka-393-2020:glosář;
  skos:prefLabel "jiná stavba"@cs;
  skos:scopeNote ""@cs .

l-sgov-sbírka-393-2020-pojem:cesta a skos:Concept;
  dcterms:source "ČSN EN 13816  - SDT";
  skos:definition "plánovaný pohyb vozidla po jediné trase"@cs;
  skos:inScheme l-sgov-sbírka-393-2020:glosář;
  skos:prefLabel "cesta"@cs;
  skos:scopeNote ""@cs .

l-sgov-sbírka-393-2020-pojem:dopravní-cesta a skos:Concept;
  dcterms:source "ČSN 018500 - SDT";
  skos:broader l-sgov-sbírka-393-2020-pojem:cesta, l-sgov-sbírka-393-2020-pojem:doprava;
  skos:definition "část prostoru určená nebo vymezená dopravě (pozemní komunikace, dráhy, vodní plochy, vzdušné prostory určené letecké dopravě)"@cs;
  skos:inScheme l-sgov-sbírka-393-2020:glosář;
  skos:prefLabel "dopravní cesta"@cs;
  skos:related l-sgov-sbírka-393-2020-pojem:inženýrská-stavba;
  skos:scopeNote ""@cs .

l-sgov-sbírka-393-2020-pojem:dráha a skos:Concept;
  dcterms:source "ZÁKON ze dne 14. prosince 1994 o dráhách § 2 (1)";
  skos:broader l-sgov-sbírka-393-2020-pojem:dopravní-cesta;
  skos:definition "cesta určená k pohybu drážních vozidel včetně pevných zařízení potřebných k zajištění bezpečnosti a plynulosti drážní dopravy"@cs;
  skos:inScheme l-sgov-sbírka-393-2020:glosář;
  skos:prefLabel "dráha"@cs;
  skos:scopeNote ""@cs .

l-sgov-sbírka-393-2020-pojem:drážní-vozidlo a skos:Concept;
  dcterms:source "vyh. 173/1995 – SDT";
  skos:altLabel "lokomotiva"@cs, "vagón"@cs;
  skos:broader l-sgov-sbírka-393-2020-pojem:vozidlo;
  skos:definition "dopravní prostředek závislý při svém pohybu na stanovené součásti dráhy (koleji, troleji, lanu ap.); drážním vozidlem není vozidlo pro technologickou obsluhu výroby, provozované na zvlášť k tomu vyhrazených kolejích vlečky, jakož i lehký dopravní prostředek, který není konstrukčně uzpůsoben pro spojování s drážními vozidly a při svém pohybu je závislý na lidské síle"@cs;
  skos:inScheme l-sgov-sbírka-393-2020:glosář;
  skos:prefLabel "drážní vozidlo"@cs;
  skos:related l-sgov-sbírka-393-2020-pojem:dráha, l-sgov-sbírka-393-2020-pojem:stavební-objekt;
  skos:scopeNote ""@cs .

l-sgov-sbírka-393-2020-pojem:vozidlo a skos:Concept;
  dcterms:source "ČSN 736100-1 – SDT,";
  skos:broader l-sgov-sbírka-393-2020-pojem:cesta, l-sgov-sbírka-393-2020-pojem:doprava;
  skos:definition "prostředek určený k přemisťování osob a/nebo nákladů buď poháněný vlastní pohonnou jednotkou nebo pohybující se pomocí lidské nebo zvířecí síly"@cs;
  skos:inScheme l-sgov-sbírka-393-2020:glosář;
  skos:prefLabel "vozidlo"@cs;
  skos:scopeNote "jedna z mnoha definic"@cs .

l-sgov-sbírka-393-2020-pojem:jiné-vozidlo a skos:Concept;
  dcterms:source "vlastní rozšíření";
  skos:definition "nedrážní vozidlo, které využívá dopravní cesty navazující na dráhu a je potřebné k udržení provozuschopnosti dráhy nebo v návaznosti na provoz dráhy (silniční nákladní a osobní vozidla, sta stavební stroje apod.)"@cs;
  skos:inScheme l-sgov-sbírka-393-2020:glosář;
  skos:prefLabel "jiné vozidlo"@cs;
  skos:scopeNote ""@cs .

l-sgov-sbírka-393-2020-pojem:obvod-dráhy a skos:Concept;
  dcterms:source "ZÁKON ze dne 14. prosince 1994 o dráhách § 2 ";
  skos:definition "území určené územním rozhodnutím pro umístění stavby dráhy"@cs;
  skos:inScheme l-sgov-sbírka-393-2020:glosář;
  skos:prefLabel "obvod dráhy"@cs;
  skos:scopeNote ""@cs .

l-sgov-sbírka-393-2020-pojem:ochranné-pásmo-dráhy a skos:Concept;
  dcterms:source "ZÁKON ze dne 14. prosince 1994 o dráhách § 2";
  skos:broader l-sgov-sbírka-393-2020-pojem:dráha;
  skos:definition "prostor po obou stranách dráhy, jehož hranice jsou vymezeny svislou plochou vedenou v zákonem stanovené vzdálenosti od krajní koleje, případně od hranic obvodu dráhy"@cs;
  skos:inScheme l-sgov-sbírka-393-2020:glosář;
  skos:prefLabel "ochranné pásmo dráhy"@cs;
  skos:scopeNote ""@cs .

l-sgov-sbírka-393-2020-pojem:stavební-objekt a skos:Concept;
  dcterms:source "SDT";
  skos:broader l-sgov-sbírka-393-2020-pojem:stavba;
  skos:definition "výsledek stavební činnosti mající charakter položky hmotného investičního majetku (s výjimkou stavebních objektů tvořících zařízení staveniště) a tvořící prostorově ucelenou nebo alespoň technicky samostatnou část stavby"@cs;
  skos:inScheme l-sgov-sbírka-393-2020:glosář;
  skos:prefLabel "stavební objekt"@cs;
  skos:scopeNote ""@cs .
