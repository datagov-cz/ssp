l-sgov-sbírka-128-2000-pojem:obec a skos:Concept;
  skos:broader z-sgov-pojem:druh, z-sgov-pojem:objekt, z-sgov-pojem:typ-objektu;
  skos:definition "Obec je základním územním samosprávným společenstvím občanů; tvoří územní celek, který je vymezen hranicí území obce."@cs;
  skos:inScheme l-sgov-sbírka-128-2000:glosář;
  skos:prefLabel "Obec"@cs .

