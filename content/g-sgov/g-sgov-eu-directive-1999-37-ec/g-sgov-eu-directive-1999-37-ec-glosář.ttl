g-sgov-eu-directive-1999-37-ec-pojem:vozidlo a skos:Concept;
  skos:inScheme g-sgov-eu-directive-1999-37-ec:glosář;
  skos:prefLabel "Vehicle"@en, "Vozidlo"@cs .

g-sgov-eu-directive-1999-37-ec-pojem:osvědčení-o-registraci a skos:Concept;
  skos:definition "Doklad osvědčující, že vozidlo je registrováno v členském státě."@cs,
    "The document which certifies that the vehicle is registered in a Member State."@en;
  skos:inScheme g-sgov-eu-directive-1999-37-ec:glosář;
  skos:prefLabel "Osvědčení o registraci"@cs, "Registration certificate"@en .

g-sgov-eu-directive-1999-37-ec-pojem:registrace a skos:Concept;
  skos:definition "Právní povolení pro uvedení vozidla do provozu na pozemních komunikacích, které zahrnuje identifikaci vozidla a vydání pořadového čísla vozidlu, zvaného registrační značka."@cs;
  skos:inScheme g-sgov-eu-directive-1999-37-ec:glosář;
  skos:prefLabel "Registrace"@cs, "Registration"@en .

g-sgov-eu-directive-1999-37-ec-pojem:osvědčuje-registraci-vozidla a skos:Concept;
  skos:inScheme g-sgov-eu-directive-1999-37-ec:glosář;
  skos:prefLabel "certifies vehicle registration"@en, "osvědčuje registraci vozidla"@cs .

g-sgov-eu-directive-1999-37-ec-pojem:držitel-osvědčení-o-registraci a skos:Concept;
  skos:definition "Osoba, na jejíž jméno je vozidlo registrováno."@cs, "The person in whose name a vehicle is registered."@en;
  skos:inScheme g-sgov-eu-directive-1999-37-ec:glosář;
  skos:prefLabel "Držitel osvědčení o registraci"@cs, "Holder of registration certificate"@en .

g-sgov-eu-directive-1999-37-ec-pojem:je-držitelem-osvědčení-o-registraci a skos:Concept;
  skos:altLabel "is holder"@en, "je držitelem"@cs;
  skos:inScheme g-sgov-eu-directive-1999-37-ec:glosář;
  skos:prefLabel "is holder of registration certificate"@en, "je držitelem osvědčení o registraci"@cs .

g-sgov-eu-directive-1999-37-ec-pojem:členský-stát a skos:Concept;
  skos:inScheme g-sgov-eu-directive-1999-37-ec:glosář;
  skos:prefLabel "Member state"@en, "Členský stát"@cs .

g-sgov-eu-directive-1999-37-ec-pojem:vydává-osvědčení-o-registraci a skos:Concept;
  skos:altLabel "issues"@en, "vydává"@cs;
  skos:inScheme g-sgov-eu-directive-1999-37-ec:glosář;
  skos:prefLabel "issue registration certificate"@en, "vydává osvědčení o registraci"@cs .

g-sgov-eu-directive-1999-37-ec-pojem:pozastavení-platnosti-povolení a skos:Concept;
  skos:definition "A limited period of time in which a vehicle is not authorised by a Member State to be used in road traffic."@en,
    "Omezená doba, kdy vozidlo není členským státem schváleno k provozu na pozemních komunikacích."@cs;
  skos:inScheme g-sgov-eu-directive-1999-37-ec:glosář;
  skos:prefLabel "Pozastavení platnosti povolení"@cs, "Suspension"@en .

g-sgov-eu-directive-1999-37-ec-pojem:schválení-vozidla-k-opětovnému-používání a skos:Concept;
  skos:definition "Pokud důvody pro pozastavení platnosti povolení pominuly, může být vozidlo schváleno k opětovnému používání, aniž by se prováděla nová registrace."@cs,
    "Provided the reasons for suspension have ceased to apply, a vehicle may be authorised to be used again without involving a new process of registration."@en;
  skos:inScheme g-sgov-eu-directive-1999-37-ec:glosář;
  skos:prefLabel "Authorisation of vehicle to be used again"@en, "Schválení vozidla k opětovnému používání"@cs .

g-sgov-eu-directive-1999-37-ec-pojem:zrušení-registrace a skos:Concept;
  skos:definition "The cancellation of a Member State’s authorisation for a vehicle to be used in road traffic."@en,
    "Zrušení schválení členského státu k provozu vozidla na pozemních komunikacích"@cs;
  skos:inScheme g-sgov-eu-directive-1999-37-ec:glosář;
  skos:prefLabel "Cancellation of registration"@en, "Zrušení registrace"@cs .

