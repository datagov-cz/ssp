@prefix a-popis-dat-pojem: <http://onto.fel.cvut.cz/ontologies/slovník/agendový/popis-dat/pojem/> .
@prefix bibo: <http://purl.org/ontology/bibo/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix g-sgov-cena: <https://slovník.gov.cz/generický/cena/> .
@prefix g-sgov-cena-pojem: <https://slovník.gov.cz/generický/cena/pojem/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix vann: <http://purl.org/vocab/vann/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix z-sgov-pojem: <https://slovník.gov.cz/základní/pojem/> .

g-sgov-cena:model a a-popis-dat-pojem:model, owl:Ontology;
  dcterms:created "2020-07-28";
  dcterms:title "Generický slovník pro ceny věcí - model"@cs;
  bibo:status "Specification"@en, "Specifikace"@cs;
  vann:preferredNamespacePrefix "g-sgov-cena-pojem";
  vann:preferredNamespaceUri "https://slovník.gov.cz/generický/cena/pojem/";
  owl:imports g-sgov-cena:glosář;
  owl:versionIRI <https://slovník.gov.cz/generický/cena/model/verze/0.0.1> .

<https://slovník.gov.cz/veřejný-sektor/pojem/časový-interval> a owl:Class .

z-sgov-pojem:je-vlastností a owl:ObjectProperty;
  owl:inverseOf z-sgov-pojem:má-vlastnost .

z-sgov-pojem:má-vlastnost a owl:ObjectProperty .

z-sgov-pojem:je-ve-vztahu a owl:ObjectProperty .

z-sgov-pojem:má-vztažený-prvek a owl:ObjectProperty;
  owl:inverseOf z-sgov-pojem:je-ve-vztahu .

z-sgov-pojem:má-vztažený-prvek-1 a owl:ObjectProperty;
  rdfs:subPropertyOf z-sgov-pojem:má-vztažený-prvek .

z-sgov-pojem:má-vztažený-prvek-2 a owl:ObjectProperty;
  rdfs:subPropertyOf z-sgov-pojem:má-vztažený-prvek .

<https://slovník.gov.cz/generický/věci/pojem/věc> a owl:Class .

g-sgov-cena-pojem:cena a owl:Class, z-sgov-pojem:typ-objektu;
  rdfs:subClassOf <https://slovník.gov.cz/generický/věci/pojem/věc>, <https://slovník.gov.cz/generický/částky/pojem/částka> .

g-sgov-cena-pojem:má-platnost a owl:Class, owl:ObjectProperty, z-sgov-pojem:typ-vztahu;
  rdfs:domain g-sgov-cena-pojem:cena;
  rdfs:range <https://slovník.gov.cz/veřejný-sektor/pojem/časový-interval>;
  rdfs:subClassOf z-sgov-pojem:vztah, [ a owl:Restriction;
      owl:onProperty z-sgov-pojem:má-vztažený-prvek-2;
      owl:someValuesFrom <https://slovník.gov.cz/veřejný-sektor/pojem/časový-interval>
    ], [ a owl:Restriction;
      owl:allValuesFrom g-sgov-cena-pojem:cena;
      owl:onProperty z-sgov-pojem:má-vztažený-prvek-1
    ], [ a owl:Restriction;
      owl:allValuesFrom <https://slovník.gov.cz/veřejný-sektor/pojem/časový-interval>;
      owl:onProperty z-sgov-pojem:má-vztažený-prvek-2
    ], [ a owl:Restriction;
      owl:onProperty z-sgov-pojem:má-vztažený-prvek-1;
      owl:someValuesFrom g-sgov-cena-pojem:cena
    ];
  rdfs:subPropertyOf z-sgov-pojem:vztah .

<https://slovník.gov.cz/generický/částky/pojem/částka> a owl:Class .

g-sgov-cena-pojem:test-term-oneee a z-sgov-pojem:typ-objektu .

skos:prefLabel rdfs:subPropertyOf rdfs:label .
